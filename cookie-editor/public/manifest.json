{
  "manifest_version": 3,
  "name": "Cookie Editor",
  "version": "1.0",
  "author": "imshaaz21",
  "description": "A Chrome extension to edit cookies.",
  "permissions": ["cookies", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
